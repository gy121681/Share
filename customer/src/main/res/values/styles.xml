<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 对话框淡入淡出动画 -->
    <style name="anim_window_fade">
        <item name="android:windowEnterAnimation">@anim/activity_top_buttom_anim</item>
        <item name="android:windowExitAnimation">@anim/activity_buttom_top_anim</item>
        <!-- <item name="android:windowEnterAnimation">@anim/window_in</item> -->
        <!-- <item name="android:windowExitAnimation">@anim/window_out</item> -->
    </style>
    <!-- 对话框底部进出动画 -->
    <style name="anim_slide_bottom">
        <item name="android:windowEnterAnimation">@anim/slide_in_from_bottom</item>
        <item name="android:windowExitAnimation">@anim/slide_out_to_bottom</item>
    </style>
    <!-- 对话框顶部进出动画 -->
    <style name="anim_slide_top">
        <item name="android:windowEnterAnimation">@anim/slide_in_from_top</item>
        <item name="android:windowExitAnimation">@anim/slide_out_to_top</item>
    </style>

    <style name="text_black">
        <item name="android:textColor">#000</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="filter_button">
        <item name="android:background">@null</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/gray_dark</item>
        <item name="android:drawableRight">@drawable/ic_down</item>
    </style>

    <style name="tab_radio_btn">
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@drawable/tab_radio_text</item>
        <item name="android:background">@drawable/tab_login_radion_bg</item>
    </style>

    <style name="tab_radio_btn2">
        <item name="android:button">@null</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@drawable/selector_text</item>
        <item name="android:background">@drawable/selector_bg</item>
    </style>

    <style name="btn_red">
        <item name="android:background">@drawable/selector_btn_pink</item>
        <item name="android:textColor">#fff</item>
        <item name="android:textSize">14sp</item>
    </style>
    <!-- btn_red -->

    <style name="btn_border_red">
        <item name="android:background">@drawable/btn_border_red</item>
        <item name="android:textColor">@color/red_dark</item>
        <item name="android:textSize">11sp</item>
    </style>

    <style name="btn_border_white">
        <item name="android:background">@drawable/btn_border_white</item>
        <item name="android:textColor">#fff</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="btn_pink">
        <item name="android:background">@drawable/selector_btn_pink</item>
        <item name="android:textColor">#fff</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="btn_orange">
        <item name="android:background">@drawable/selector_btn_orange</item>
        <item name="android:textColor">#fff</item>
        <item name="android:textSize">13sp</item>
    </style>



    <style name="btn_gray">
        <item name="android:background">@drawable/btn_gray</item>
        <item name="android:textColor">#fff</item>
        <item name="android:textSize">13sp</item>
    </style>

    <style name="btn_search">
        <item name="android:layout_width">200dp</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:gravity">left|center_vertical</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:background">@drawable/bg_arc_white</item>
        <item name="android:drawableLeft">@drawable/share_c_homepage_search_icon</item>
        <item name="android:drawablePadding">6dp</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/gray_light</item>
    </style>

    <style name="rb_red">
        <item name="android:background">#fff</item>
        <item name="android:button">@null</item>
        <item name="android:drawableRight">@drawable/cb_red</item>
        <item name="android:textSize">14sp</item>
        <item name="android:padding">10dp</item>
    </style>

    <style name="ratingbar_style" parent="@android:style/Widget.RatingBar">
        <item name="android:progressDrawable">@drawable/rating_bar</item>
        <item name="android:numStars">5</item>
        <item name="android:stepSize">1</item>
        <item name="android:rating">3</item>
        <item name="android:minHeight">22dp</item>
        <item name="android:maxHeight">25dp</item>
    </style>

    <!-- 加载条dialog样式 -->
    <style name="CustomDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowSoftInputMode">adjustPan</item>
    </style>

    <style name="LicenseDialogStyle"  parent="@android:style/Theme.Dialog">

        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">false</item>
        <!--<item name="android:backgroundDimEnabled">true</item>-->
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
    </style>

    <style name="Translucent">
        <item name="android:windowNoTitle">true</item>
        <item name="android:fitsSystemWindows">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 设置全屏 -->
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowFullscreen">false</item>
        <item name="android:windowAnimationStyle">@style/AnimationActivity</item>
    </style>

    <style name="mydialog" >
