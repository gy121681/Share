<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/lin_1"
        style="@style/title_container_theme" >

        <TextView
            android:id="@+id/bt_title_left"
            style="@style/title_back_btn_bg_theme2"
            android:drawableLeft="@drawable/title_back_btn_bg2"
            android:text="返回" />

        <CheckBox
            android:id="@+id/cb_title_contre"
            style="@style/title_text_theme"
            android:button="@drawable/profite_checkbox"
            android:text="2016" />
    </RelativeLayout>

    <ListView
        android:id="@+id/mylist"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:divider="#000000"
        android:listSelector="#00000000"
        android:scrollbars="none" >
    </ListView>

</LinearLayout>