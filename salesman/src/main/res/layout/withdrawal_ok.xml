<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/absoluteLayoutRoot"
    android:layout_width="308.0dip"
    android:layout_height="wrap_content"
    android:background="@drawable/common_dialog_bg" >

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <RelativeLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical" >

            <TextView
                android:id="@+id/common_dialog_title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="@dimen/margin_half"
                android:layout_marginTop="2.0dip"
                android:gravity="center"
                android:text="提示"
                android:textColor="#ff4e4e4e"
                android:textSize="20.000977sp" />

            <RelativeLayout
                android:id="@+id/layout_common_dialog_msg_line_bold"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="40.0dip" >

                <ImageView
                    android:id="@+id/wechat_pay_open_ico"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:src="@drawable/wechat_pay_open_ico" />

                <TextView
                    android:id="@+id/common_dialog_msg_line_bold"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:layout_marginLeft="@dimen/margin_half"
                    android:layout_toRightOf="@id/wechat_pay_open_ico"
                    android:gravity="center"
                    android:text="提现成功"
                    android:textColor="#ff414d5b"
                    android:textSize="18.0sp" />
            </RelativeLayout>

            <TextView
                android:visibility="gone"
                android:id="@+id/common_dialog_msg_line_normal"
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/layout_common_dialog_msg_line_bold"
                android:layout_centerHorizontal="true"
                android:layout_marginLeft="@dimen/margin_half"
                android:layout_marginRight="@dimen/margin_half"
                android:layout_marginTop="@dimen/margin_normal"
                android:gravity="center"
                android:text="您现在可以返回用微信支付收款啦"
                android:textColor="#ff424f58"
                android:textSize="15.000977sp" />

            <RelativeLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_below="@id/common_dialog_msg_line_normal"
                android:layout_marginTop="@dimen/margin_half"
                android:paddingBottom="5.0dip" >

                <Button
                    android:id="@+id/buttonOk"
                    android:layout_width="100.0dip"
                    android:layout_height="40.0dip"
                    android:layout_centerInParent="true"
                    android:layout_marginLeft="9.299988dip"
                    android:layout_marginRight="9.299988dip"
                    android:background="@drawable/common_next_button_selector"
                    android:gravity="center"
                    android:text="确定"
                    android:textColor="#ffffffff"
                    android:textSize="18.299988sp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="10.0dip"
                    android:layout_marginTop="50.0dip"
                    android:gravity="center"
                    android:textColor="#ff6b6b6b"
                    android:textSize="18.299988sp" />
            </RelativeLayout>
        </RelativeLayout>
    </LinearLayout>

</RelativeLayout>