<?xml version="1.0" encoding="UTF-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <style name="navigation_title">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#FFFFFFFF</item>
        <item name="android:textSize">20dip</item>
        <item name="android:layout_centerInParent">true</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:gravity">center_horizontal</item>
    </style>

    <style name="Dialog_Fullscreen">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <!-- 将中间件以窗口样式运行所用的style -->
    <style name="Dialog_fullscream_activity" parent="android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="dialog_elflocation" parent="android:style/Theme.Dialog">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name="MyDialog" parent="@android:style/Theme.Dialog">
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@drawable/alert_bg</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>


    <style name="LicenseDialogStyle"  parent="@android:style/Theme.Dialog">

        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">false</item>
        <!--<item name="android:backgroundDimEnabled">true</item>-->
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowCloseOnTouchOutside">true</item>
    </style>

    <!-- 全局title的layout样式 -->
    <style name="title_container_theme">
        <item name="android:background">@color/bg_white</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">49.0dip</item>
    </style>

    <!-- 全局title的layout样式 -->
    <style name="gif">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>

    <!-- 全局title的返回样式 -->
    <style name="title_back_btn_bg_theme">
        <item name="android:background">@null</item>
        <item name="android:paddingLeft">15.0dip</item>
        <item name="android:paddingRight">15.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:drawableLeft">@drawable/title_back_btn_bg</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <!-- 全局titel的中间内容样式 -->
    <style name="title_text_theme">
        <item name="android:textSize">@dimen/text_38</item>
        <item name="android:textColor">@color/text_black</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_centerInParent">true</item>
    </style>

    <!-- 全局titel的确认样式 -->
    <style name="title_btn_bg_theme">
        <item name="android:textSize">@dimen/size_16</item>
        <item name="android:textColor">@drawable/txt_fg_white_selector</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@null</item>
        <item name="android:paddingLeft">15.0dip</item>
        <item name="android:paddingRight">15.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:text">确定</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>

    <!-- 全局返回样式二 -->
    <style name="title_back_btn_bg_theme2">
        <item name="android:textSize">@dimen/text_28</item>
        <item name="android:textColor">@color/text_black</item>
        <item name="android:gravity">center</item>
       
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minWidth">50.0dip</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
<!--      <item name="android:background">@drawable/title_back_btn_bg2</item> -->

    <!-- 全局确认样式二 -->
    <style name="title_btn_bg_theme2">
        <item name="android:textSize">14.0dip</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:gravity">left</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minWidth">50.0dip</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    
        <style name="title_btn_bg_theme3">
        <item name="android:textSize">14.0dip</item>
        <item name="android:textColor">@color/text_black</item>
        <item name="android:gravity">left</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">35dp</item>
        <item name="android:minWidth">50.0dip</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

    <!--
    <style name="PopupAnimation" parent="@android:style/Animation">
        <item name="android:windowEnterAnimation">@anim/popup_enter</item>
        <item name="android:windowExitAnimation">@anim/popup_exit</item>
    </style>



    -->


    <!-- 从下往上弹出DIALOG -->
    <style name="TransparentDialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@style/dialog_from_bottom_anim</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>

    <!-- dialog底部弹出效果 -->
    <style name="dialog_from_bottom_anim">
        <item name="android:windowEnterAnimation">@anim/translate_visible_from_bottom</item>
        <item name="android:windowExitAnimation">@anim/translate_gone_into_bottom</item>
    </style>

    <!-- 注册父类布局style -->
    <style name="register_parrent">
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">50.0dip</item>
    </style>
    <!-- 注册右边style -->
    <style name="register_right_style">
        <item name="android:background">@drawable/register_right_middle</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">3.0</item>
    </style>
    <!-- 注册edittext的style -->
    <style name="register_right_edittext">
        <item name="android:textSize">16.0dip</item>
        <item name="android:textColor">@color/tv_register_right_text</item>
        <item name="android:textColorHint">@color/tv_register_right_hint</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">right|center</item>
        <item name="android:background">@color/transparents</item>
        <item name="android:paddingRight">10.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:singleLine">true</item>
    </style>
    <!-- 错误TextView的Style -->
    <style name="register_right_error">
        <item name="android:textSize">12.0dip</item>
        <item name="android:textColor">@color/tv_register_right_error</item>
        <item name="android:gravity">right|center</item>
        <item name="android:background">@drawable/inputerror_middle_bg</item>
        <item name="android:paddingRight">10.0dip</item>
        <item name="android:visibility">gone</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <!-- 左边标题 -->
    <style name="register_title_tip">
        <item name="android:textSize">14.0dip</item>
        <item name="android:textColor">@color/tv_register_left_tip</item>
        <item name="android:gravity">right|center</item>
        <item name="android:background">@drawable/register_left_middle</item>
        <item name="android:paddingRight">15.0dip</item>
        <item name="android:layout_width">0.0dip</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1.6</item>
    </style>

    <style name="register_right_lay_with_noerror" parent="@style/register_right_style">
        <item name="android:gravity">center</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:paddingRight">10.0dip</item>
    </style>

    <style name="text_style_large_bold">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="Theme.UPPay" parent="android:style/Theme.Light">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="text_style_18_bold">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="text_style_15_bold">
        <item name="android:textSize">15.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/black</item>
    </style>

    <style name="text_style_18_bold_white">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="mStyle" parent="@android:style/Widget" />

    <style name="mStyle.blue12_left" parent="@style/mStyle">
        <item name="android:textSize">12.0dip</item>
        <item name="android:textColor">@color/regBlue</item>
        <item name="android:gravity">left|center</item>
    </style>

    <style name="mStyle.reg_content_title" parent="@style/mStyle">
        <item name="android:textSize">@dimen/reg_tv_textsize</item>
        <item name="android:textColor">@color/content_title</item>
        <item name="android:gravity">center</item>
    </style>
    
    
    
    <style name="popwin_anim_style">  
     <item name="android:windowEnterAnimation">@anim/anim_enter</item>  
</style> 

</resources>